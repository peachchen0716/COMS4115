# addition on float
float a = 1.0 + 2.0;

if (a == 3.0){
	print(1);
}
else {
	print(0);
}

# subtract on float 
float b = -10.0 + 5.0;

if (b == -5.0){
	print(1);
}
else{
	print(0);
}

# times on float 
float c = 11.1 * 6.0;

if (c == 66.6){
	print(1);
}
else{
	print(0);
}

# divide on float 
float d = 12.0;
float e = 2.0;
e = d / e;

if (e == 6.0){
	print(1);
}
else{
	print(0);
}



